#%Module

proc ModulesHelp { } {
    puts stderr "This module sets the path and environment variables for petsc-3.14.6"
    puts stderr "     see https://www.mcs.anl.gov/petsc/ for more information      "
    puts stderr ""
}
module-whatis "PETSc - Portable, Extensible Toolkit for Scientific Computation"

set petsc_dir   "/cygdrive/c/checkouts/modflow/petsc-3.14.6"
set petsc_arch  "arch-mswin-c-opt"

setenv PETSC_ARCH "$petsc_arch"
setenv PETSC_DIR "$petsc_dir"
prepend-path PATH "/cygdrive/c/checkouts/modflow/petsc-3.14.6/arch-mswin-c-opt/bin:/cygdrive/c/checkouts/modflow/petsc-3.14.6/bin"
